msgstr "Die maximale unmittelbare Geschwindigkeitsänderung während des Glättens."

#: fdmprinter.def.json
msgctxt "jerk_support label"
msgid "Support Jerk"
msgstr "Ruckfunktion Stützstruktur"

#: fdmprinter.def.json
msgctxt "jerk_support description"
