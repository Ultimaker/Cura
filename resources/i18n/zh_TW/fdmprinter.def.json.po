msgstr ""

msgctxt "retraction_enable label"
msgid "Enable Retraction"
msgstr "啟用回抽"

msgctxt "support_brim_enable label"
msgid "Enable Support Brim"
msgstr "啟用支撐邊緣"

msgctxt "support_bottom_enable label"
