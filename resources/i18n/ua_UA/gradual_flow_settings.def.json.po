msgctxt "gradual_flow_enabled label"
msgid "Gradual flow enabled"
msgstr "Плавний контроль увімкнено"

msgctxt "gradual_flow_enabled description"
msgid "Enable gradual flow changes. When enabled, the flow is gradually increased/decreased to the target flow. This is useful for printers with a bowden tube where the flow is not immediately changed when the extruder motor starts/stops."
msgstr "Увімкнути зміну плавного контролю. Коли увімкнено потік постійно збільшується/зменшується до цільового значення. Це корисно для принтерів з трубкою Боудена, де потік не міняється відразу при запуску/зупинці двигуна екструдера."

msgctxt "max_flow_acceleration label"
msgid "Gradual flow max acceleration"
msgstr "Максимальне прискорення плавного потоку"

msgctxt "max_flow_acceleration description"
msgid "Maximum acceleration for gradual flow changes"
msgstr "Максимальне прискорення для зміни плавного потоку"

msgctxt "layer_0_max_flow_acceleration label"
msgid "Initial layer max flow acceleration"
msgstr "Максимальне прискорення на початковому шарі"

msgctxt "layer_0_max_flow_acceleration description"
msgid "Minimum speed for gradual flow changes for the first layer"
msgstr "Мінімальна швидкість зміни плавного потоку для першого шару"

msgctxt "gradual_flow_discretisation_step_size label"
msgid "Gradual flow discretisation step size"
msgstr "Розмір кроку дискретизації плавного потоку"

msgctxt "gradual_flow_discretisation_step_size description"
msgid "Duration of each step in the gradual flow change"
msgstr "Тривалість кожного кроку в зміні плавного потоку"

msgctxt "reset_flow_duration label"
msgid "Reset flow duration"
msgstr "Скинути плавний потік"

msgctxt "reset_flow_duration description"
msgid "For any travel move longer than this value, the material flow is reset to the paths target flow"
msgstr "Для будь-якого переміщення довше ніж це значення, потік матеріалу скидається до шляху цілі потоку"