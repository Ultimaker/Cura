msgid "Prime Tower Jerk"
msgstr "Velocitat de viratge de la torre de preparació"

#: fdmprinter.def.json
msgctxt "jerk_prime_tower description"
msgid ""
"The maximum instantaneous velocity change with which the prime tower is "
"printed."
msgstr ""
"El canvi instantani màxim de velocitat amb la qual s'imprimeix la torre de "
"preparació."

#: fdmprinter.def.json
msgctxt "jerk_travel label"
msgid "Travel Jerk"
msgstr "Velocitat de viratge de desplaçament"

#: fdmprinter.def.json
msgctxt "jerk_travel description"
msgid ""
"The maximum instantaneous velocity change with which travel moves are made."
msgstr ""
"El canvi instantani màxim de velocitat amb la qual es fan moviments de "
"desplaçament."

#: fdmprinter.def.json
msgctxt "jerk_layer_0 label"
msgid "Initial Layer Jerk"
msgstr "Velocitat de viratge de la capa inicial"

#: fdmprinter.def.json
msgctxt "jerk_layer_0 description"
msgid "The print maximum instantaneous velocity change for the initial layer."
msgstr ""
"El canvi instantani màxim de velocitat d'impressió per a la capa inicial."

#: fdmprinter.def.json
