msgstr "ラフト土台の追加マージン"

msgctxt "raft_base_extruder_nr label"
msgid "Raft Base Extruder"
msgstr "ラフト土台用エクストルーダー"

msgctxt "raft_base_fan_speed label"
msgid "Raft Base Fan Speed"
msgstr "ラフト土台ファン速度"

