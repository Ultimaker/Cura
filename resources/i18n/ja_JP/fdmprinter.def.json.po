msgid "Wall Extruder"
msgstr "ウォールエクストルーダー"

#: fdmprinter.def.json
msgctxt "wall_material_flow label"
msgid "Wall Flow"
msgstr "壁のフロー"

#: fdmprinter.def.json
msgctxt "jerk_wall label"
msgid "Wall Jerk"
msgstr "ウォールジャーク"

#: fdmprinter.def.json
msgctxt "wall_line_count label"
msgid "Wall Line Count"
msgstr "ウォールライン数"

#: fdmprinter.def.json
msgctxt "wall_line_width label"
msgid "Wall Line Width"
msgstr "ウォールライン幅"

#: fdmprinter.def.json
